{
  "name": "@mysten/walrus",
  "version": "0.7.0",
  "private": false,
  "description": "Walrus SDK",
  "license": "Apache-2.0",
  "author": "Mysten Labs <build@mystenlabs.com>",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "CHANGELOG.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mystenlabs/ts-sdks.git"
  },
  "bugs": {
    "url": "https://github.com/mystenlabs/ts-sdks/issues"
  },
  "homepage": "https://sdk.mystenlabs.com/walrus",
  "devDependencies": {
    "@hono/node-server": "^1.19.1",
    "@tanstack/react-query": "^5.87.1",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.2",
    "hono": "^4.9.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.2",
    "undici": "^7.15.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "@mysten/build-scripts": "0.0.0",
    "@mysten/codegen": "0.5.0",
    "@mysten/dapp-kit-core": "0.0.1",
    "@mysten/dapp-kit-react": "0.0.1"
  },
  "dependencies": {
    "dataloader": "^2.2.3",
    "@mysten/bcs": "1.8.0",
    "@mysten/sui": "1.38.0",
    "@mysten/utils": "0.2.0",
    "@mysten/walrus-wasm": "0.1.1"
  },
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "build": "build-package",
    "codegen": "sui-ts-codegen generate && pnpm lint:fix",
    "prettier:check": "prettier -c --ignore-unknown .",
    "prettier:fix": "prettier -w --ignore-unknown .",
    "eslint:check": "eslint --max-warnings=0 .",
    "eslint:fix": "pnpm run eslint:check --fix",
    "lint": "pnpm run eslint:check && pnpm run prettier:check",
    "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
    "dev:write-from-wallet": "vite examples/write-from-wallet",
    "dev:benchmark": "vite examples/benchmark"
  }
}